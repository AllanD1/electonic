{
  "dependencies": {
    "angular2": "2.0.0-beta.13",
    "es6-promise": "3.0.2",
    "es6-shim": "^0.35.0",
    "ionic-angular": "2.0.0-beta.4",
    "ionic-native": "^1.1.0",
    "ionicons": "3.0.0-alpha.3",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "zone.js": "0.6.6"
  },
  "devDependencies": {
    "del": "2.2.0",
    "electron-packager": "^7.0.0",
    "electron-prebuilt": "^0.37.6",
    "gulp": "3.9.1",
    "gulp-watch": "4.3.5",
    "ionic-gulp-browserify-es2015": "^1.0.2",
    "ionic-gulp-fonts-copy": "^1.0.0",
    "ionic-gulp-html-copy": "^1.0.0",
    "ionic-gulp-sass-build": "^1.0.0",
    "ionic-gulp-scripts-copy": "^1.0.0",
    "run-sequence": "1.1.5"
  },
  "cordovaPlugins": [
    "cordova-plugin-device",
    "cordova-plugin-console",
    "cordova-plugin-whitelist",
    "cordova-plugin-splashscreen",
    "cordova-plugin-statusbar",
    "ionic-plugin-keyboard"
  ],
  "cordovaPlatforms": [
    "ios",
    {
      "platform": "ios",
      "version": "1.0.0",
      "locator": "ios"
    }
  ],
  "main": "electron/main.js",
  "scripts": {
    "dev": "electron electron/dev.js -dev",
    "test": "electron electron/dev.js",

    "clear:darwin": "rm -rf *-darwin-x64",

    "pack:darwin": "npm run clear:darwin && electron-packager ./ --platform=darwin --arch=x64 --ignore=\"/electron/dev.js\" --ignore=\"/app/\" --ignore=\"/config.xml\" --ignore=\"/gulpfile.js\" --ignore=\"/hooks/\" --ignore=\"/ionic.config.json\" --ignore=\"/node_modules/\" --ignore=\"/README.md\" --ignore=\"/resources/\""
  },
  "name": "Electonic",
  "packageName": "Electonic",
  "version": "1.0.0",
  "description": "a boilerplate combining electron, ionic2 and cordova for creating truly cross platform apps using web technologies"
}
